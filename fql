select object_id, caption, backdated_time, created, place_id  FROM photo WHERE (pid IN (SELECT pid FROM photo_tag WHERE subject='544095626') OR pid IN (SELECT pid FROM photo WHERE aid IN (SELECT aid FROM album WHERE owner='544095626' AND type!='profile')) or owner = 544095626)
and created > 1339457709 and place_id <> ''
order by created 


